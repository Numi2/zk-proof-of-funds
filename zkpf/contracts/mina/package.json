{
  "name": "@zkpf/mina-contracts",
  "version": "0.1.0",
  "description": "Mina zkApp contracts for zkpf recursive proof verification and cross-chain attestations",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "setup": "node --loader ts-node/esm src/setup.ts keys",
    "setup:local": "node --loader ts-node/esm src/setup.ts local",
    "setup:testnet": "node --loader ts-node/esm src/setup.ts testnet",
    "setup:compile": "node --loader ts-node/esm src/setup.ts compile",
    "deploy": "node --loader ts-node/esm src/deploy.ts",
    "deploy:devnet": "MINA_NETWORK=devnet node --loader ts-node/esm src/deploy.ts",
    "test:local": "node --loader ts-node/esm src/test-local.ts",
    "test:add": "node --loader ts-node/esm src/Add.test.ts",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "o1js": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "mina",
    "zkapp",
    "zkpf",
    "proof-of-funds",
    "zero-knowledge"
  ]
}
