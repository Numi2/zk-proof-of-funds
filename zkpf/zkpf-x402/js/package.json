{
  "name": "@numi2/x402-zec",
  "version": "0.1.0",
  "description": "x402 Payment Required client SDK for Zcash (ZEC) payments - Accept ZEC in your API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/react.tsx --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/react.tsx --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build",
    "test": "echo 'No tests yet'",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "x402",
    "zcash",
    "zec",
    "payment",
    "http",
    "cryptocurrency",
    "privacy",
    "shielded",
    "402",
    "payment-required",
    "api",
    "monetization"
  ],
  "author": "Numan Thabit <numi2>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/numi2/x402-zec"
  },
  "homepage": "https://github.com/numi2/x402-zec#readme",
  "bugs": {
    "url": "https://github.com/numi2/x402-zec/issues"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}

